<div class="events">
  <% @events.each do |event| %>
  <%= link_to event_path(event) do%>
  <div class="card-event">
      <img src="https://images.unsplash.com/photo-1503177119275-0aa32b3a9368?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1470&q=80" />
     <div class="card-event-infos">
        <h5>
        <strong><%= event.name %></strong>
       </h5>
       <p><%= event.category %></p>
       <p><%= event.era %></p>
       <p><%= event.year %></p>
       <p><%= event.price %></p>
        <p><%= event.location %></p>
    </div>
  </div>
  <% end %>
  <% end %>
</div>
